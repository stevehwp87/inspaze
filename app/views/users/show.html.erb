

<div class="container">
	<div class="col-md-12 col-md-offset-0">
		<%= link_to "Edit Profile", edit_user_path , :class => "btn btn-default pull-right"%>
		<%= link_to "Account Setting", edit_user_registration_path , :class => "btn btn-default pull-right"%>	
	</div>

	  <div class="col-md-9 col-md-offset-3">

			

				<%= form_for(@user) do |f| %>
					<div class="form-group">
						<div class="col-md-4">
							<%= f.label :first_name %>
						</div>
						<div class="col-md-8">
							<%= @user.first_name %>
						</div>
					</div>

					<div class="form-group">
						<div class="col-md-4">
							<%= f.label :last_name %>
						</div>
						<div class="col-md-8">
							<%= @user.last_name %>
						</div>
					</div>

					

					<div class="form-group">
						<div class="col-md-4">
							<%= f.label :date_of_birth %>
						</div>
						<div class="col-md-8">
						    <%= @user.date_of_birth %>
						</div>
					</div>


					<div class="form-group">
						<div class="col-md-4">
						  <%= f.label :gender %>
						</div>
						<div class="col-md-8">
							<%= @user.gender %>
						</div>
					</div>

					<div class="form-group">
						<div class="col-md-4">
							<%= f.label :state %>
						</div>
						<div class="col-md-8">
							<%= @user.state %>
						</div>
					</div>

					<div class="form-group">
						<div class="col-md-4">
							<%= f.label :country %>
						</div>
						<div class="col-md-8">
							<%= @user.country %>
						</div>
					</div>

					<div class="form-group">
						<div class="col-md-4">
							<%= f.label :phone_no %>
						</div>
						<div class="col-md-8">
							<%= @user.phone_no %>
						</div>
					</div>

				<% end %>	
				
	  </div>
  </div>